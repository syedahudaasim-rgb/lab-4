#include <stdio.h>

int main() {
    int m1, m2, m3, m4, m5;
    int total;
    float percentage;

    printf("Enter marks of 5 subjects: ");
    scanf("%d %d %d %d %d", &m1, &m2, &m3, &m4, &m5);

    total = m1 + m2 + m3 + m4 + m5;
    percentage = total / 5.0;

    printf("Percentage = %.2f\n", percentage);

    if (percentage >= 85)
        printf("Grade A");
    else if (percentage >= 70)
        printf("Grade B");
    else if (percentage >= 50)
        printf("Grade C");
    else
        printf("Fail");

    return 0;
}

